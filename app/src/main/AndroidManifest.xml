            android:label="Flush Door" />
        <activity
            android:name=".RoundLog"
            android:label="Round Log" />
        <activity
            android:name=".CutSize"
            android:label="Cut Size" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>